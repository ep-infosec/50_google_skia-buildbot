{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://bugs.skia.org",
        "type": 301
      },
      {
        "source": "/:bug*",
        "destination": "https://bugs.skia.org/:bug",
        "type": 301
      }
    ]
  }
}
